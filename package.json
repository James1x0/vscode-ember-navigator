{
  "name": "vscode-ember-navigator",
  "displayName": "Ember Navigator",
  "description": "A productivity tool for Ember.js developers.",
  "version": "1.2.0",
  "publisher": "oreoz",
  "repository": "https://github.com/Oreoz/vscode-ember-navigator",
  "engines": {
    "vscode": "^1.24.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:handlebars",
    "onCommand:emberNavigator.openEmberNavigator",
    "onCommand:emberNavigator.toggleJavaScriptHandlebars",
    "onCommand:emberNavigator.navigateToActiveComponent"
  ],
  "main": "src/extension",
  "contributes": {
    "commands": [
      {
        "command": "emberNavigator.openEmberNavigator",
        "title": "Ember Navigator"
      },
      {
        "command": "emberNavigator.toggleJavaScriptHandlebars",
        "title": "Ember Navigator: Toggle JavaScript / Handlebars"
      },
      {
        "command": "emberNavigator.navigateToActiveComponent",
        "title": "Ember Navigator: Navigate to Active Component"
      },
      {
        "command": "emberNavigator.toggleRouteController",
        "title": "Ember Navigator: Toggle Route / Controller"
      },
    ],
    "keybindings": [
      {
        "command": "emberNavigator.openEmberNavigator",
        "key": "ctrl+[Enter]",
        "mac": "cmd+[Enter]",
        "when": "editorTextFocus"
      },
      {
        "command": "emberNavigator.toggleJavaScriptHandlebars",
        "key": "ctrl+alt+e",
        "mac": "shift+cmd+e",
        "when": "editorTextFocus"
      },
      {
        "command": "emberNavigator.toggleRouteController",
        "key": "ctrl+alt+r",
        "mac": "shift+cmd+r",
        "when": "editorTextFocus"
      },
      {
        "command": "emberNavigator.navigateToActiveComponent",
        "key": "ctrl+alt+[Enter]",
        "mac": "shift+cmd+[Enter]",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.6.1",
    "vscode": "^1.1.6",
    "eslint": "^4.11.0",
    "@types/node": "^7.0.43",
    "@types/mocha": "^2.2.42"
  }
}
